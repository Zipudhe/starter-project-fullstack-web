{
  "name": "starter-project",
  "version": "1.0.0",
  "description": "Monorepo of my starter project for fullstack web projects using React.js, Express.js, Sequelize & PostgreSQL",
  "main": "index.js",
  "repository": "git@github.com:rafael-g-depaulo/starter-project-express-react-sequelize-pgsql.git",
  "author": "Rafael G. de Paulo <rafael.g.depaulo@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "client",
    "server",
    "strapi"
  ],
  "scripts": {
    "build": "yarn && yarn build:client && yarn build:server",
    "build:client": "yarn && yarn --cwd client build",
    "build:server": "yarn && yarn --cwd server build",
    "clean": "yarn clean:client && yarn clean:server",
    "clean:client": "rm -rf ./client/build && mkdir ./client/build",
    "clean:server": "rm -rf ./server/build && mkdir ./server/build",
    "start": "yarn start:server",
    "start:server": "yarn --cwd server start",
    "start:strapi": "yarn --cwd strapi start",
    "start:client": "yarn --cwd client start"
  },
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  }
}
