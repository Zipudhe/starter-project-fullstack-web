{
  "name": "@starter-project/client",
  "version": "4.2.0",
  "main": "dist/client.cjs.js",
  "license": "MIT",
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "if-env": "^1.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "serve": "^11.3.2",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "dotenv -- if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:dev": "craco start",
    "start:prod": "yarn serve",
    "build": "craco build && cp -p ./serve.json ./build/serve.json",
    "test": "craco test",
    "eject": "craco eject",
    "serve": "serve ./build -l ${PORT:-4000} -s -n"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "devDependencies": {
    "babel-jest": "^26.2.2"
  }
}
