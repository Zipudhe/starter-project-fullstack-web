{
  "name": "@starter-project/api-typescript",
  "version": "5.1.0",
  "description": "express.js api in typescript",
  "keywords": [
    "api",
    "server",
    "typescript"
  ],
  "author": "Rafael G. de Paulo <rafael.g.depaulo@gmail.com>",
  "homepage": "https://github.com/rafael-g-depaulo/starter-project-express-react-sequelize-pgsql#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "typings": "dist/index.d.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafael-g-depaulo/starter-project-express-react-sequelize-pgsql.git"
  },
  "scripts": {
    "start": "dotenv -- if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "dev": "yarn start:dev",
    "prod": "yarn start:prod",
    "start:dev": "dotenv -- tsnd -r tsconfig-paths/register --project ./tsconfig.json --respawn --transpile-only src/index.ts",
    "start:prod": "dotenv -- yarn serve",
    "serve": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc && link-module-alias",
    "clean": "rm -fr dist; rm -f .ultra.cache.json; rm -f tsconfig.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/rafael-g-depaulo/starter-project-express-react-sequelize-pgsql/issues"
  },
  "dependencies": {
    "@starter-project/adder": "5.1.0",
    "@types/express": "^4.17.8",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "link-module-alias": "^1.2.0"
  },
  "devDependencies": {
    "tsconfig-paths": "^3.9.0"
  },
  "_moduleAliases": {
    "Middlewares": "dist/Middlewares"
  }
}
